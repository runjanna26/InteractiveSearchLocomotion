        <geom name="ramp" type="box" size="3.0 1.0 0.1" pos="1.5 0 1.265" euler="0 -15 0" 
                rgba="0.6 0.4 0.2 1" friction="1.5 0.005 0.0001"/>

        <!-- Light settings -->
        <light directional="true" diffuse=".4 .4 .4" specular="0.1 0.1 0.1" pos="0 0 5.0"
            dir="0 0 -1" castshadow="false" />
        <light directional="true" diffuse=".6 .6 .6" specular="0.2 0.2 0.2" pos="0 0 4" dir="0 0 -1" />

        <!-- <body mocap="true" pos="-.1 .05 0" zaxis=".5 0 1"></body> -->
    </worldbody>


    <actuator>
        <motor name="TR0" joint="TR0" ctrllimited="true" ctrlrange="-10 10"/>
