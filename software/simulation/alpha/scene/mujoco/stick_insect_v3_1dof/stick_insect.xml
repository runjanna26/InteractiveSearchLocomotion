        <!-- Water Space -->
        <geom name="water_space" type="box" size="4 4 1" pos="-2 -2 0.0" 
            rgba="0 0.5 1 0.2" conaffinity="0" contype="0"/>

        <!-- Environment Ground -->
        <!-- <geom name="ground_0" type="box" size="2 2 0.05" pos="0 0 1.1" quat="1 0 0 0" material="matplane"
            condim="1" friction="1.5 0.005 0.0001" contype="1"/> -->

        <geom name="ground_1" type="box" size="2 2 1" pos="4 0 0.5" quat="1 0 0 0" material="matplane"
